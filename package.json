{
  "name": "samira",
  "version": "1.0.3",
  "description": "A TypeScript library for League of Legends API calls",
  "author": "Gabriel-Passoss",
  "homepage": "https://github.com/Gabriel-Passoss/Samira#readme",
  "bugs": "https://github.com/Gabriel-Passoss/Samira/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabriel-Passoss/Samira.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts",
    "lint:prettier": "prettier -c .",
    "format:prettier": "prettier -w ."
  },
  "keywords": [
    "league-of-legends",
    "lol",
    "api",
    "typescript",
    "zod"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@types/node": "^20.8.0",
    "@vitest/ui": "^1.0.0",
    "dotenv": "^16.3.1",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
